<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="GoTo">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>GoTo</Description>
          <Count>20</Count>
        </Item>
      </Statistic>
      <Statistic Action="Messages">
        <Item>
          <IdeaActionID>ActivateMessagesToolWindow</IdeaActionID>
          <ShortCut>'⌘0'</ShortCut>
          <Description>Messages</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'GroupSyncConfigTest.should launch step groupSync (1)'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌃F5'</ShortCut>
          <Description>Rerun 'GroupSyncConfigTest.should launch step groupSync (1)'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Podcast-Server'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧F10'</ShortCut>
          <Description>Run 'Podcast-Server'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Full Unit'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Full Unit'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Podcast-Server'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Podcast-Server'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>