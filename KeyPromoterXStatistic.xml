<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'⌥⏎'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'⌘+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Backend'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'⇧F9' or '⌃⇧⌘R'</ShortCut>
          <Description>Debug 'Backend'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide">
        <Item>
          <ShortCut>'⇧⎋'</ShortCut>
          <Description>Hide</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Backend'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌃F5'</ShortCut>
          <Description>Rerun 'Backend'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert">
        <Item>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌘+4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Backend'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Backend'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>