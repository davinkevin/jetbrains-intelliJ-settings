<filetype binary="false" description="Docker build file" name="Dockerfile">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="#" />
      <option name="COMMENT_START" value="" />
      <option name="COMMENT_END" value="" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
    </options>
    <keywords keywords="ADD;CMD;COPY;ENTRYPOINT;ENV;EXPOSE;FROM;MAINTAINER;ONBUILD;RUN;USER;VOLUME;WORKDIR" ignore_case="false" />
    <keywords2 keywords="add-apt-repository;addgroup;adduser;apt-get;cd;chmod;chown;cp;curl;dpkg;dpkg-divert;echo;ln;mkdir;mv;printf;rm;rsync;sed;tar;wget" />
    <keywords3 keywords="install;update;upgrade" />
    <keywords4 keywords="+x;--disabled-password;--force-yes;--gecos;--ingroup;--quiet;-O;-R;-e;-f;-fr;-i;-q;-rf;-y" />
  </highlighting>
  <extensionMap>
    <mapping pattern="Dockerfile" />
  </extensionMap>
</filetype>