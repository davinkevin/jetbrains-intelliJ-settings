        <added pattern="org.springframework.web.util.UriComponentsBuilder.pathSegment(pathSegments)" />
        <added pattern="org.assertj.core.api.AbstractObjectAssert.isEqualToComparingOnlyGivenFields(other,propertiesOrFieldsUsedInComparison)" />
        <added pattern="lan.dk.podcastserver.manager.downloader.DownloadingItem(item,urls,filename,userAgent)" />
        <added pattern="java.io.File(pathname)" />
        <added pattern="com.nhaarman.mockitokotlin2.times(numInvocations)" />
        <added pattern="com.airbusds.intelligence.subscriptionmanagement.subscription.SubscriptionV2(uid,name,activated,startDate,endDate,configuration,contract,service,createdAt,createdBy,updatedAt,updatedBy)" />
        <added pattern="com.airbusds.intelligence.subscriptionmanagement.group.GroupV2(uid,label,createdAt,createdBy,updatedAt,updatedBy,contract)" />
