        <added pattern="kotlin.text.replace(oldValue,newValue,ignoreCase)" />
        <added pattern="reactor.test.StepVerifier.FirstStep.expectNext(t1,t2,t3,t4)" />
        <added pattern="java.time.ZonedDateTime.of(year,month,dayOfMonth,hour,minute,second,nanoOfSecond,zone)" />
      </blacklist>
    </blacklists>
  </component>
</application>